{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "http://github.com/o19s/ubi/schema/query.response.schema.json",
  "title": "Query Response",
  "description": "The response to a query made by a user.",
  "type": "object",
  "required": [ "query_id" ],
  "properties": {  
    "query_id": {    
      "description": "The unique identifier of a query, typically a GUID.",    
      "type": "string"
    },
    "query_response_objects_ids":{
      "description": "Objects returned by the query",
      "type": "array",
      "items": {
        "anyOf": [
          {
            "type": "string"
          },
          {
            "type": "integer"
          }
        ]
      }
    }
  }
}
